{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "8.0.0"
                }
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-export-default-from",
        [
            "@babel/plugin-transform-runtime",
            {
                "helpers": false,
                "regenerator": true,
                "corejs": 3
            }
        ]
    ]
}
